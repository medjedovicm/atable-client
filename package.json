{
  "name": "atable-frontend",
  "version": "0.0.8",
  "main": "./src/electron-main.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "localDev": "ng serve --configuration=localDev",
    "remoteDev": "ng serve --configuration=remoteDev",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build": "ng build --prod --aot",
    "release": "standard-version",
    "deploy": "sshpass scp -o stricthostkeychecking=no -r ./dist/* intreksc@intreks.com:/home/intreksc/atable.intreks.com",
    "release-deploy": "npm run release && npm run build && npm run deploy",
    "package-linux": "electron-packager . atable-frontend --overwrite --platform=linux --arch=x64 --icon=./src/assets/icons/atable_icon.ico --prune=true --out=release-builds",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 ---icon=./src/assets/icons/atable_icon.ico --prune=true --out=release-builds",
    "package-win": "electron-packager . atable-frontend --overwrite --asar=true --platform=win32 --arch=ia32 --icon=./src/assets/icons/atable_icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"atable-frontend\"",
    "create-debian-installer": "electron-installer-debian --src release-builds/atable-linux-x64/ --arch amd64 --config debian.json",
    "electron": "electron .",
    "electron-build": "ng build --base-href ./ && electron ."
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.1.0",
    "@angular/cdk": "^9.2.0",
    "@angular/common": "~9.1.0",
    "@angular/compiler": "~9.1.0",
    "@angular/core": "~11.0.5",
    "@angular/forms": "~9.1.0",
    "@angular/material": "^9.2.0",
    "@angular/platform-browser": "~9.1.0",
    "@angular/platform-browser-dynamic": "~9.1.0",
    "@angular/router": "~9.1.0",
    "@auth0/angular-jwt": "^2.1.1",
    "@ncstate/sat-popover": "^4.0.0",
    "@tinymce/tinymce-angular": "^3.5.0",
    "bootstrap": "^4.3.1",
    "electron": "^8.2.4",
    "electron-installer-debian": "^3.0.0",
    "electron-packager": "^14.2.1",
    "express": "^4.17.1",
    "hammerjs": "^2.0.8",
    "howler": "^2.1.3",
    "moment": "^2.24.0",
    "ngx-bootstrap": "^5.5.0",
    "ngx-cookie-service": "^2.2.0",
    "ngx-image-cropper": "^3.1.5",
    "node-notifier": "^7.0.0",
    "path": "^0.12.7",
    "rxjs": "~6.5.4",
    "socket.io-client": "^2.3.0",
    "standard-version": "^8.0.1",
    "tslib": "^1.11.1",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.0",
    "@angular/cli": "~9.1.0",
    "@angular/compiler-cli": "~9.1.0",
    "@angular/language-service": "~9.1.0",
    "@fortawesome/fontawesome-free": "^5.9.0",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.12.37",
    "codelyzer": "^5.0.0",
    "electron": "^8.2.4",
    "electron-packager": "^14.2.1",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.8.3"
  },
  "engines": {
    "node": "10.16.0",
    "npm": "6.9.0"
  }
}
